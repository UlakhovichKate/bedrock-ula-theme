<div class="{{ class }}__items feed">
    {% for post in items %}
        {% include ['components/tease/tease-' ~ post.post_type ~'.twig', 'components/tease/tease.twig'] %}
    {% endfor %}

    {% include 'components/partial/pagination.twig' with { pagination: posts.pagination({show_all: false, mid_size: 3, end_size: 2}) } %}
</div>
